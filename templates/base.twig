{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body>
		<header class="header" >
			{% block header %}
				<div class="wrapper">
					<nav class="bg-gray-900 ">
						<div class="flex py-1 mx-2 justify-between lg:container lg:mx-auto ">
							<div class="text-white">
								<a class="text-2xl font-semibold">Lucio Torres</a>
							</div>
							<div class="">
								{% include "menu.twig" with {'items': menu.get_items} %}
							</div>
						</div>
   					</nav>
				</div>
			{% endblock %}
		</header>

		
				{% block content %}
				<section id="content" role="main" class="content-wrapper">
					<div class="wrapper {{sidebar_class}}">
						Sorry, no content
					</div>
				</section>
				{% endblock %}
			
			{% if sidebar %}
				<aside class="layout-sidebar">
					{{sidebar}}
				</aside>
			{% endif %}
		</section>

		{% block footer %}
			<footer id="footer">
				{% include 'footer.twig' %}
			</footer>
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>
